<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                                       http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">

    <changeSet id="createDivipolTable" author="manuel.motta">
        <createTable tableName="DIVIPOL">
            <column name="DIPOCONS" type="NUMBER">
                <constraints nullable="true" />
            </column>
            <column name="DEPARTAMENTO" type="NUMBER(2)">
                <constraints nullable="false" />
            </column>
            <column name="MUNICIPIO" type="NUMBER(3)">
                <constraints nullable="false" />
            </column>
            <column name="ZONA" type="NUMBER(2)">
                <constraints nullable="false" />
            </column>
            <column name="PUESTO" type="CHAR(2)">
                <constraints nullable="false" />
            </column>
            <column name="DEPARTAMENTO_DESCRIPCION" type="VARCHAR2(50)">
                <constraints nullable="false" />
            </column>
            <column name="MUNICIPIO_DESCRIPCION" type="VARCHAR2(50)">
                <constraints nullable="false" />
            </column>
            <column name="PUESTO_DESCRIPCION" type="VARCHAR2(150)">
                <constraints nullable="false" />
            </column>
            <column name="POT_HOMBRE" type="NUMBER(10)">
                <constraints nullable="false" />
            </column>
            <column name="POT_MUJER" type="NUMBER(10)">
                <constraints nullable="false" />
            </column>
            <column name="TOTAL" type="NUMBER(10)">
                <constraints nullable="false" />
            </column>
            <column name="MESAS" type="NUMBER(3)">
                <constraints nullable="false" />
            </column>
        </createTable>

    </changeSet>

</databaseChangeLog>
